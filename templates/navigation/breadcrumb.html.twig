{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}
{% if breadcrumb %}
  {% embed 'umdds:umd-element-breadcrumb' %} 
    {% block breadcrumbs_slot %}
      {% for item in breadcrumb %}
        {% if item.url %}
          <a href="{{ item.url }}">{{ item.text }}</a>
        {% else %}
          {{ item.text }}
        {% endif %}
      {% endfor %}
    {% endblock %} 
  {% endembed %}
{% endif %}