<umd-element-pathway image-position="left" type="{{ pathway_type }}">
  <div slot="image" data-caption="{{ pathway_image_caption }}">
    <img src="{{ pathway_image }}" alt="{{ pathway_image_alt }}">
  </div>

  {% if eyebrow %}  
    <p slot="eyebrow">{{ eyebrow }}</p>
  {% endif %}
  <h2 slot="headline">{{ headline }}</h2>
  <div slot="text">
    {{ body }}
  </div>
    
  <div slot="actions">
    {% block cta_slot %}{% endblock %}
  </div>
</umd-element-pathway>
