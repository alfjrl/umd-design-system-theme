<umd-element-card>
  <img slot="image" src="{{ card_image }}"
    alt="{{ card_image_alt }}" />
  <div slot="eyebrow">
    <p>{{ eyebrow }}</p>
  </div>
  <div slot="headline">
    <h3>
      {{ headline }}
    </h3>
  </div>
  <div slot="text">
    <p>
      {{ body }}
    </p>
  </div>
  {% if element_date %}
    <div slot="date">
      <time datetime="{{ element_date|date("Y-m-d") }}">{{ element_date|date('F j, Y') }}</time>
    </div>
  {% endif %}
</umd-element-card>
